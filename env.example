# Hugging Face API Configuration
# Get your free API key from: https://huggingface.co/settings/tokens
# Copy this file to .env and replace YOUR_API_KEY_HERE with your actual key

REACT_APP_HF_API_KEY=YOUR_API_KEY_HERE

# Groq API Configuration
# Get your free API key from: https://console.groq.com/keys
# This is used for AI monster analysis and AI-powered prompt generation

REACT_APP_GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE

# Multiplayer Server Configuration
# URL for the multiplayer server (defaults to http://localhost:3002)
REACT_APP_SERVER_URL=http://localhost:3002 